{
  "name": "my-electron-vite-vue3",
  "private": true,
  "version": "0.0.0",
  "electronMain": "./electron/main.ts",
  "main": "./dist/electron/main.js",
  "scripts": {
    "dev": "vite & npm run nodemon",
    "nodemon": "export NODE_ENV='development' && nodemon --exec electron transpileModule.js --watch ./electron --ext .ts",
    "build": "vite build && tsc -p ./electron && electron-builder"
  },
  "build": {
    "files": [
      "./dist/**/*"
    ],
    "extends": null
  },
  "dependencies": {
    "@types/node": "^18.8.2",
    "axios": "^1.0.0",
    "dotenv": "^16.0.3",
    "electron-win-state": "^1.1.22",
    "less": "^4.1.3",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.1.0",
    "electron": "^21.0.1",
    "electron-builder": "^23.3.3",
    "nodemon": "^2.0.20",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vue-tsc": "^0.40.4"
  }
}